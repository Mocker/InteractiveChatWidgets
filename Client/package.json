{
  "name": "interactive-chat-widgets",
  "version": "0.0.1",
  "description": "A framework to create interactive widgets that overlay popular video chat platforms",
  "main": "src/main.ts",
  "scripts": {
    "build": "tsc src/main.ts",
    "prestart": "npm run build",
    "start": "electron src/main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "set NODE_ENV=development&&electron .",
    "tsnd": "tsnd --respawn src/main.ts",
    "devmon": "nodemon --watch src/*.js --exec electron src/main.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalktalk": "kenperlin/chalktalk",
    "dgram": "^1.0.1",
    "express": "^4.17.1",
    "formidable": "^1.2.2",
    "http": "0.0.1-security",
    "path": "^0.12.7",
    "ws": "^7.4.0"
  },
  "devDependencies": {
    "electron": "^10.1.5",
    "electron-reloader": "^1.0.1",
    "nodemon": "^2.0.6",
    "ts-node-dev": "^1.0.0"
  }
}
